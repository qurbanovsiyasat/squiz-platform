{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "strict": false,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": false,
    "exactOptionalPropertyTypes": false
  },
  "include": ["src"]
}
